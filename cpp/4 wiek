#include <iostream>

using namespace std;

int age;

int main()
{
    cout << "Twoj wiek" << endl;
    cin >> age;
    if (age>=18)
        cout << "Jestes pelnoletni i ";
    else cout << "Nie jestes pelnoletni i ";
    if (age>35)
        cout << "mozesz zostac prezydentem";
    else cout << "nie mozesz zostac prezydentem";
    return 0;
}
